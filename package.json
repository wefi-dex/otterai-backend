{
  "name": "otterai-sales-analytics-backend",
  "version": "1.0.0",
  "description": "Backend API for OtterAI Sales Analytics SaaS Platform",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "lint": "eslint src/",
    "migrate": "node database/run-migration.js migrate",
    "migrate:rollback": "node database/run-migration.js rollback",
    "migrate:status": "node database/run-migration.js status",
    "seed": "node src/database/seed.js",
    "setup-zapier": "node scripts/setup-zapier.js",
    "db:create": "node database/setup-database.js create",
    "db:drop": "node database/setup-database.js drop",
    "db:reset": "node database/setup-database.js reset && npm run migrate",
    "db:setup": "node database/setup-database.js setup",
    "db:test": "node database/setup-database.js test",
    "db:fix": "node scripts/fix-database-connection.js",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:reload": "pm2 reload ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "pm2:status": "pm2 status"
  },
  "keywords": [
    "otterai",
    "sales",
    "analytics",
    "saas",
    "express",
    "nodejs"
  ],
  "author": "Patrick Higgin",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.879.0",
    "@aws-sdk/s3-request-presigner": "^3.879.0",
    "@sendgrid/mail": "^8.1.5",
    "aws-sdk": "^2.1490.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.1.5",
    "express-slow-down": "^2.0.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "pg": "^8.16.3",
    "redis": "^4.6.10",
    "sequelize": "^6.35.1",
    "sharp": "^0.32.6",
    "socket.io": "^4.7.4",
    "twilio": "^4.19.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
